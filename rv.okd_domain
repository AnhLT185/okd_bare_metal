$TTL 604800
@ IN SOA okd-services.<OKD_DOMAIN>. admin.<OKD_DOMAIN>. (
    6       ; Serial
    604800  ; Refresh
    86400   ; Retry
    2419200 ; Expire
    604800  ; Negative Cache TTL
)

; Name servers - "NS" records.
 IN NS okd-services.<OKD_DOMAIN>.

; Name servers - "PTR" records.
<OKD_SERVICES_LST_OCT> IN PTR okd-services.<OKD_DOMAIN>.

; OpenShift container platform cluster - "PTR" records.
<OKD_BOOTSTRAP_LST_OCT> IN PTR okd-bootstrap.mbr.<OKD_DOMAIN>.
<OKD_MASTER_1_LST_OCT> IN PTR okd-master-1.mbr.<OKD_DOMAIN>.
<OKD_MASTER_2_LST_OCT> IN PTR okd-master-2.mbr.<OKD_DOMAIN>.
<OKD_MASTER_3_LST_OCT> IN PTR okd-master-3.mbr.<OKD_DOMAIN>.
<OKD_WORKER_1_LST_OCT> IN PTR okd-worker-1.mbr.<OKD_DOMAIN>.
<OKD_WORKER_2_LST_OCT> IN PTR okd-worker-2.mbr.<OKD_DOMAIN>.
<OKD_SERVICES_LST_OCT> IN PTR api.mbr.<OKD_DOMAIN>.
<OKD_SERVICES_LST_OCT> IN PTR api-int.mbr.<OKD_DOMAIN>.
